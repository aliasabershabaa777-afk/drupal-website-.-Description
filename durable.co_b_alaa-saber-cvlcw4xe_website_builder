input[type="checkbox"]:checked:focus { --tw-ring-color: rgba(0,0,0,var(--tw-ring-opacity)); --tw-ring-opacity: 0.8; }

input[type="checkbox"]:disabled { cursor: default; opacity: 0.5; }

